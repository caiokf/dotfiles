{
  "cSpell.language": "en,en-GB,en-US,ru",
  "cSpell.showStatus": false,
  "cSpell.diagnosticLevel": "Hint",
  "debug.allowBreakpointsEverywhere": true,
  "editor.accessibilitySupport": "off",
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.folding": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.cursorSurroundingLines": 3,
  "editor.useTabStops": true,
  "editor.formatOnPaste": false,
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    "*.pyi": "python",
    "zshrc": "shellscript",
    ".aliases": "shellscript",
    ".exports": "shellscript",
    ".prompt": "shellscript",
    ".functions": "shellscript",
    "bash_profile": "shellscript",
    "editorconfig": "yaml",
    "Fastfile": "ruby",
    "Deliverfile": "ruby",
    "Appfile": "ruby",
    "Matchfile": "ruby"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.idea": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/.coverage": true,
    "**/*.py[co]": true
  },
  "git.autofetch": false,
  "python.formatting.provider": "none",
  "python.pythonPath": "${workspaceFolder}/.venv/bin/python3",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.env.osx": {
    "SOBOLE_THEME_MODE": "dark"
  },
  "terminal.integrated.enableBell": true,
  "window.clickThroughInactive": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "gitDecoration.modifiedResourceForeground": "#d4cb68",
    "activityBar.activeBorder": "#e5b358",
    "editorSuggestWidget.highlightForeground": "#e5b358",
    "editorSuggestWidget.selectedBackground": "#1c2a42",
  },
  "workbench.colorTheme": "Black Ocean",
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "ayu",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tips.enabled": false,
  "python.linting.enabled": false,
  "git.showInlineOpenFileAction": false,
  "git.autorefresh": true,
  "git.decorations.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.suggest.autoImports": false,
  "typescript.suggest.autoImports": false,
  "vetur.completion.autoImport": false,
  "vetur.experimental.templateInterpolationService": true,
  "breadcrumbs.enabled": false,
  "go.formatTool": "goimports",
  "javascript.updateImportsOnFileMove.enabled": "always",

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "gitlens.mode.active": "zen",
}
